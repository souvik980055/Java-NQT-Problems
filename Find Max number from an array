class LargestElement {
    public static int LargestElement(int arr[]){
        int Max = 0;
        for(int i=0; i<arr.length; i++){
            
            if(arr[i]>Max){
                Max = arr[i];
            }
        }
        return Max;
    }
    
    public static void main(String[] args) {
        int arr[] = {50, 30, 60, 20, 40, 20};
        //selectionSort(arr);
        System.out.println(LargestElement(arr));
    }
}

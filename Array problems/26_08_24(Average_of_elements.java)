public class AverageElements {
    public static void averageElements(int[] arr) {
        int n=arr.length;
        int sum=0, ave;
        for(int i=0; i<arr.length; i++){
            sum += arr[i];
        }
        ave = sum/n;
        System.out.println(ave);
    }
    public static void main(String[] args) {
        int arr[]={34, 56, 67, 35};
        averageElements(arr);
    }
}
